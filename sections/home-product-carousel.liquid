 #shopify-section-{{section.id}}.home-product-carousel:before{

    content: " ";
    position: absolute;
    width: 100%;
    
    
    mask-repeat: repeat-x;
    mask-size: 100%;
      {% if section.settings.before_image != blank  %}
    mask-image:url({{ section.settings.before_image | img_url:'master' }});
    -webkit-mask-image:url({{ section.settings.before_image | img_url:'master' }});

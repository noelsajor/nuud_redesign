{% assign margin = section.settings.margin | split: ',' %}
{% assign margin_top = margin[0] %}
{% assign margin_bottom = margin[1] %} 
{% assign padding = section.settings.padding | split: ',' %}
{% assign padding_top = padding[0] %}
{% assign padding_bottom = padding[1] %} 

{% if section.blocks.size > 0 %}   
{% assign ColumnWidth = section.settings.content_width | split: '/' %}
{% assign leftColumn = ColumnWidth[0] %}
{% assign rightColumn = ColumnWidth[1] %}

{% assign listWidth = section.settings.lap_content_width | split: '/' %}
{% assign lap_leftColumn = listWidth[0] %}
{% assign lap_rightColumn = listWidth[1] %}
{% endif %}

{% case section.settings.grid %}
{% when '1' %}
{% when '2' %}
{%- assign column = 'two-column' -%}
{% when '3' %}
{%- assign column = 'three-column' -%}              
{% when '4' %}
{%- assign column = 'four-column' -%}
{% when '5' %}
{%- assign column = 'five-column' -%}
{% when '6' %}
{%- assign column = 'six-column' -%}
{% else %}
{%- assign column = 'three-column' -%}
{% endcase %}


{% if section.settings.enable_overlay %}
{% assign section_overlay = 'dt-sc-overlay' %}
{% endif %}

{%- assign collection = collections[section.settings.collection] -%}
{% if section.settings.show_background_image %}
{% assign lazyScript = "lazyload" %}
{% endif %}
<div class="dt-sc-section-wrapper  {{lazyScript}} {{ section_overlay }} {% if section.settings.background_style == 'parallax' %} parallax-initiated{% endif %}" {% if section.settings.background_style == 'parallax' %} data-speed="2" data-style="parallax"{% endif %} style="{% render 'bg-color-image-padding' %} background-size: {{- section.settings.background_size -}}; {% if section.settings.background_style == 'fixed' %}background-attachment: fixed;{% endif %} background-repeat:{% if section.settings.background_size == 'repeat' %}repeat{% else %}no-repeat;{% endif %}" {% include 'bg_image' %}>  
    <div class="leftimage" style="{% if section.settings.image_1 != blank %} background-image:url('{{ section.settings.image_1 | img_url: 'master' }}');background-repeat:no-repeat; position:absolute;width:385px;height:365px;z-index: 0;{% endif %}"></div>

  <div class="{% render 'container-settings' %}"> 
    <div class="row {{ section.settings.custom_class_name }}">
      {% render 'section-heading', class: section.id %} 
      {% if section.blocks.size > 0 %} <div class="grouped-content{% if section.settings.reverse_column %} dt-sc-reverse-columns{% endif %}">{% endif %}
      {% if section.blocks.size > 0 %}            
      <div class="additional-block">
        {% for block in section.blocks %}       

        {% if block.settings.block_banner_style == 'grid' %}
        {% assign grid_style = 'grid-style' %}
        {% elsif block.settings.block_banner_style == 'overlay' %}
        {% assign overlay_style = 'overlay-style' %}
        {% endif %}

        {% case  block.type %}
        {% when 'menu' %}
        <div class="dt-sc-menu-section">
          {% assign blockMenu = linklists[block.settings.menu] %}
          {% if block.settings.block_title != blank %}
          <h3 class="additional-grid-title">{{block.settings.block_title}}</h3>
          {% endif %}
          <ul class="menu-links">
            {% for link in blockMenu.links %}
            <li {% if link.current %}class="active"{% endif %}><a href="{{ link.url}}">{{ link.title }}</a></li>
            {% endfor %}
          </ul>
        </div>
        {% when 'banner' %}
        <div class="dt-sc-grid-banner-section">
          <div class="dt-sc-grid-banner {{overlay_style}} {{block.type}}-{{block.id}}">      
            {% if block.settings.show_image %}
            <div class="dt-sc-grid-banner-image {{block.settings.banner_image_overlay_style}}">
              {% if block.settings.block_image != blank %}
              {%- assign img_url = block.settings.block_image | img_url: 'master' | replace: '_1x1.', '_{width}x.' -%}
              <img class="dt-sc-brand-image lazyload"
                   src="{{ 'loading.gif' | asset_url }}"
                   data-src="{{ img_url }}"
                   data-widths="[180, 360, 470, 600, 770, 970, 1060, 1280, 1512, 1728, 2048]"
                   data-aspectratio="{{ block.settings.block_image.aspect_ratio }}"
                   data-sizes="auto"            
                   data-alt="{{ block.settings.block_image.alt }}">
              <noscript>
                {{ block.settings.block_image | img_url: '480x480', scale: 2 | img_tag: block.settings.block_image.alt, 'dt-sc-noscript-image' }}
              </noscript>
              {% else %}
             {% capture current %}{% cycle 1, 2, 3, 4, 5, 6 %}{% endcapture %}
          {{ 'product-' | append: current | placeholder_svg_tag: 'placeholder-svg dark' }}
              {% endif%}                 
            </div>
            {% endif%} 
            {% if block.settings.block_banner_style == 'grid' %}
            {% if block.settings.block_title != blank or block.settings.block_sub_title != blank or   block.settings.block_description != blank or block.settings.block_button_link != blank %}
            <div class="dt-sc-grid-banner-content {{block.settings.block_text_align}}">            
              {% if block.settings.block_title != blank %}
              <h4 class="dt-sc-main-title">{{block.settings.block_title}}</h4>
              {% endif %}
              {% if block.settings.block_sub_title != blank %}
              <h6 class="dt-sc-sub-title">{{block.settings.block_sub_title}}</h6>
              {% endif %}
              {% if block.settings.block_description != blank %}
              <p class="dt-sc-description">{{block.settings.block_description}}</p>
              {% endif %}
              {% if block.settings.block_button_link != blank %}
              <a href="{{block.settings.block_button_link}}" class="dt-sc-btn">{{block.settings.block_button_text}}</a>
              {% endif %}            
            </div>
            {% endif%} 
            {% endif %}     
            {% if block.settings.block_banner_style == 'overlay' %}
            {% if block.settings.block_title != blank or block.settings.block_sub_title != blank or   block.settings.block_description != blank or block.settings.block_button_link != blank %}
            <div class="dt-sc-grid-banner-content {{block.settings.content_position}}">            
              <div class="dt-sc-grid-banner-inner {{block.settings.block_text_align}}">
                {% if block.settings.block_title != blank %}
                <h4 class="dt-sc-main-title">{{block.settings.block_title}}</h4>
                {% endif %}
                {% if block.settings.block_sub_title != blank %}
                <h6 class="dt-sc-sub-title">{{block.settings.block_sub_title}}</h6>
                {% endif %}
                {% if block.settings.block_description != blank %}
                <p class="dt-sc-description">{{block.settings.block_description}}</p>
                {% endif %}
                {% if block.settings.block_button_link != blank %}
                <a href="{{block.settings.block_button_link}}" class="dt-sc-btn">{{block.settings.block_button_text}}</a>
                {% endif %} 
              </div>
            </div>
            {% endif %}     
            {% endif %} 
          </div>
        </div>
        {% when 'timer' %}
        <div class="dt-sc-grid-banner-section deals-section">
          <div class="dt-sc-grid-banner {{overlay_style}} {{block.type}}-{{block.id}}">      
            {% if block.settings.timer_block_image %}
            <div class="dt-sc-grid-banner-image {{block.settings.banner_image_overlay_style}}">
              {% if block.settings.block_image != blank %}
              <img class="lazyload"
                   src="{{ 'loading.gif' | asset_url }}"
                   data-src="{{ block.settings.block_image | img_url: 'master' }}"
                   data-widths="[180, 360, 470, 600, 770, 970, 1060, 1280, 1512, 1728, 2048]"
                   data-aspectratio="{{ block.settings.block_image.aspect_ratio }}"
                   data-sizes="auto"            
                   data-alt="{{ block.settings.block_title }}">
              <noscript>
                {{ block.settings.block_image | img_url: '800x800', scale: 2 | img_tag: block.settings.block_image.alt, 'dt-sc-noscript-image' }}
              </noscript>
              {% else %}
              {% capture current %}{% cycle 1, 2, 3, 4, 5, 6 %}{% endcapture %}
          {{ 'product-' | append: current | placeholder_svg_tag: 'placeholder-svg dark' }}
              {% endif%} 
            </div>
            {% endif%} 
            {% if block.settings.block_title != blank or block.settings.block_sub_title != blank or   block.settings.deal_end_date != blank or  block.settings.block_description != blank or block.settings.block_button_link != blank %}
            <div class="dt-sc-grid-banner-content {{block.settings.block_text_align}}">
              {% if block.settings.block_title != blank %}
              <h4 class="dt-sc-main-title">{{block.settings.block_title}}</h4>
              {% endif %}
              {% if block.settings.block_description != blank %}
              <p class="dt-sc-description">{{block.settings.block_description}}</p>
              {% endif %}
              <div class="deals-counter">
                {% if block.settings.block_sub_title != blank %}
                <h6 class="dt-sc-sub-title">{{block.settings.block_sub_title}}</h6>
                {% endif %}
                {% if block.settings.deal_end_date != blank %}
                {% assign endTime = block.settings.deal_end_date | date: "%d %b %Y" %}
                <div class="product-deal-count" data-end-time="{{endTime}}">
                  <div class="notice"></div>
                  <div class="deal-clock">
                    <ul>
                      <li class="days"></li>
                      <li class="hours"></li>
                      <li class="minutes"></li>
                      <li class="seconds"></li>
                    </ul>
                  </div>
                </div>                
                {% endif %}
              </div>
              {% if block.settings.block_button_link != blank %}
              <a href="{{block.settings.block_button_link}}" class="dt-sc-btn">{{block.settings.block_button_text}}</a>
              {% endif %} 
            </div> 
            {% endif%} 
          </div>
        </div>

        {% when 'product' %}          
        <div class="wow animated fadeInDown dT_vDynamicPWrap-{{ section.id }}-block-{{ block.id}} dT_vProdWrap">
          <ul>
            {%- assign product = all_products[block.settings.product] -%}       
            {% include 'product-grid-item', product: product, swiperSlideClass: 'carousel-block-item' %}         
          </ul>
        </div>
        {% when 'collection' %}          
        {% assign block_collection = collections[block.settings.collection] %}
        {% if block.settings.block_title != blank %}
        <h3 class="additional-grid-title">{{block.settings.block_title}}</h3>
        {% endif %}
        {% if block.settings.enable_slider %}
        <div data-section-id="blockCarousel" data-section-type="home-blockCarousel-section"
             data-item-per-view="{{ block.settings.items_per_view }}"
             data-laptop-screen-items="{{ block.settings.items_laptop_view }}"
             data-tablet-screen-items  ="{{ block.settings.items_tablet_view }}"
             data-small-screen-items="{{block.settings.items_mobile_view}}"
             data-autoplay="{{block.settings.slider_autoplay}}"
             data-blocks-limit="{{section.blocks.size}}"
             data-item-space="{{block.settings.collection_column_gap}}"
             data-center-mode="{{block.settings.enable_center_mode }}"            
             data-item-row="{{ block.settings.rows }}"
             data-auto-height="false">       
          <div class="dt-sc-swiper-slider swiper-container wow animated fadeInDown {% if block.settings.dots %} {{ block.settings.carousel_pagination_alignment }} {% endif %} {% if block.settings.arrows %}  {{ block.settings.nav_arrow_position }} {{ block.settings.center_nav_arrow_alignment }} {% endif %}">
            <div id="swiper-blockCarousel-slider" class="dT_vDynamicPWrap-{{ section.id }}-block-{{ block.id}} dT_vProdWrap">         
              <ul class="swiper-wrapper product-list-style wow animated fadeInDown">                          
                {% assign  swiperSlideClass = "swiper-slide" %}                 
                {% for product in block_collection.products limit: 9 %}                  
                {% include 'product-grid-item', product: product, swiperSlideClass: swiperSlideClass %} 
                   {% else %}
              {% for i in (1..4) %}
                   {%- assign placeholder_image = 'product-apparel-' | append: forloop.rindex -%}
              {% include 'empty-row' , placeholder_image: placeholder_image %} 
              {% endfor %}  
                {% endfor %}
              </ul>
              {% if block.settings.arrows %}                    
              <div class="swiper-blockCarousel-arrows swiper-arrows">
                <div id="swiper-blockCarousel-next" class="swiper-button-next dt-sc-btn"></div>
                <div id="swiper-blockCarousel-prev" class="swiper-button-prev dt-sc-btn"></div>
              </div>  
              {% endif  %} 
              {% if block.settings.dots %}
              <div id="swiper-blockCarousel-pagination" class="swiper-pagination"></div>
              {% endif %}
            </div>
          </div>

        </div> 
        {% else %}
        <div data-section-id="blockCarousel" data-section-type="home-blockCarousel-section">
          <div class="dT_vDynamicPWrap-{{ section.id }}-block-{{ block.id}} dT_vProdWrap">
            <ul class="product-list-style">         
              {% assign  swiperSlideClass = "carousel-block-list" %}                
              {% for product in block_collection.products  limit: block.settings.limit %}                  
              {% include 'product-grid-item', product: product, swiperSlideClass: swiperSlideClass %} 
              {% endfor %}
            </ul>
          </div>
        </div>
        {% endif %}
        {% endcase %}
        {% endfor %}                

      </div>
      {% endif %}

      {% if section.settings.enable_main_content != blank%}
      <div class="main-block">        
        {% unless section.settings.enable_slider %} 
        <div data-section-id="blockCarousel" data-section-type="home-blockCarousel-section">
          <div class="dT_vDynamicPWrap-{{ section.id }} dT_vProdWrap">
            <ul class="grid product-collection dt-sc-column {{column}} {% if section.settings.enable_list_view %}product-list-style{% endif %}">         
              {% for product in collection.products limit: section.settings.post_limit  %}                  
              {% include 'product-grid-item', product: product %}
              {% else %}
              {% for i in (1..4) %}
                   {%- assign placeholder_image = 'product-apparel-' | append: forloop.rindex -%}
              {% include 'empty-row' , placeholder_image: placeholder_image %} 
              {% endfor %}
              {% endfor %}
            </ul>
          </div>
        </div>
        {% endunless %}

        {% if section.settings.enable_slider %}
        <div data-section-id="{{ section.id }}" data-section-type="home-product-carousel-section" 
             data-item-per-view="{{section.settings.items_per_view}}"
             data-laptop-screen-items="{{section.settings.items_laptop_view}}"
             data-tablet-screen-items  ="{{section.settings.items_tablet_view}}"
             data-small-screen-items="{{section.settings.items_mobile_view}}"
             data-center-mode="{{section.settings.enable_center_mode }}"             
             data-autoplay="{{section.settings.slider_autoplay}}"   data-blocks-limit="{{section.blocks.size}}" data-item-space="{{section.settings.column_gap}}"  data-item-row="{{ section.settings.rows }}" >       
          <div class="dt-sc-swiper-slider swiper-container {% if section.settings.dots %} {{ section.settings.carousel_pagination_bottom_alignment }} {{ section.settings.carousel_pagination_alignment }} {% endif %} {% if section.settings.arrows %}  {{ section.settings.nav_arrow_position }} {{ section.settings.center_nav_arrow_alignment }} {% endif %}">
            <div id="swiper-{{section.id}}-slider" class="dT_vDynamicPWrap-{{ section.id }} dT_vProdWrap">  
              <ul class="swiper-wrapper {% if section.settings.enable_list_view %}product-list-style{% endif %}">         
                {% for product in collection.products  %}                  
                {% include 'product-grid-item', product: product, swiperSlideClass: 'swiper-slide' %} 
                {% else %}
                {% for i in (1..4) %}
               {%- assign placeholder_image = 'product-apparel-' | append: forloop.rindex -%}
              {% render 'empty-row' , placeholder_image: placeholder_image %}
                {% endfor %}
                {% endfor %}
              </ul>
            </div>  
            {% if section.settings.arrows %}                    
            <div class="swiper-{{ section.id}}-arrows swiper-arrows">
              <div id="swiper-{{ section.id}}-next" class="swiper-button-next dt-sc-btn"></div>
              <div id="swiper-{{ section.id}}-prev" class="swiper-button-prev dt-sc-btn"></div>
            </div>  
            {% endif  %} 
            {% if section.settings.dots %}
            <div id="swiper-{{ section.id}}-pagination" class="swiper-pagination"></div>
            {% endif %}
          </div>
        </div>  
        {% endif %}                    

      </div>
      {% endif %}    


      {% if section.blocks.size > 0 %}</div>{% endif %}             
    </div>
  </div>
   {% comment %} <div class="rightimage" style="{% if section.settings.image_2 != blank %} background-image:url('{{ section.settings.image_2 | img_url: 'master' }}');background-repeat:no-repeat; position:absolute;width:500px;height:800px;z-index: 0;{% endif %}"></div>{% endcomment %}
</div> 

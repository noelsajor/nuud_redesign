{% doc %}
  @prompt
    A costumer reviews section which allows me to add a full review card, or an just image card, or a video card, I want to be able to change the color of the background of the entire section, the section background color selector is not working, padding adjustment is not working

{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-reviews-section-{{ ai_gen_id }} {
    padding: {{ block.settings.section_padding_top }}px 0 {{ block.settings.section_padding_bottom }}px;
    background-color: {{ block.settings.section_background_color }};
  }

  .ai-reviews-container-{{ ai_gen_id }} {
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 {{ block.settings.section_padding_horizontal }}px;
  }

  .ai-reviews-header-{{ ai_gen_id }} {
    text-align: center;
    margin-bottom: 40px;
  }

  .ai-reviews-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.title_size }}px;
    color: {{ block.settings.title_color }};
    margin: 0 0 16px;
  }

  .ai-reviews-subtitle-{{ ai_gen_id }} {
    font-size: {{ block.settings.subtitle_size }}px;
    color: {{ block.settings.subtitle_color }};
    margin: 0;
  }

  .ai-reviews-grid-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
    gap: {{ block.settings.card_gap }}px;
  }

  .ai-review-card-{{ ai_gen_id }} {
    background-color: {{ block.settings.card_background }};
    border-radius: {{ block.settings.card_border_radius }}px;
    padding: {{ block.settings.card_padding_vertical }}px {{ block.settings.card_padding_horizontal }}px;
    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
    transition: transform 0.3s ease, box-shadow 0.3s ease;
  }

  .ai-review-card-{{ ai_gen_id }}:hover {
    transform: translateY(-4px);
    box-shadow: 0 8px 15px rgba(0, 0, 0, 0.15);
  }

  .ai-review-card-image-only-{{ ai_gen_id }} {
    padding: 0;
    overflow: hidden;
  }

  .ai-review-card-video-only-{{ ai_gen_id }} {
    padding: 0;
    overflow: hidden;
  }

  .ai-review-header-{{ ai_gen_id }} {
    display: flex;
    align-items: center;
    gap: 12px;
    margin-bottom: 16px;
  }

  .ai-review-avatar-{{ ai_gen_id }} {
    width: 50px;
    height: 50px;
    border-radius: 50%;
    overflow: hidden;
    flex-shrink: 0;
  }

  .ai-review-avatar-{{ ai_gen_id }} img {
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .ai-review-avatar-placeholder-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    background-color: {{ block.settings.placeholder_color }};
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .ai-review-avatar-placeholder-{{ ai_gen_id }} svg {
    width: 24px;
    height: 24px;
    color: {{ block.settings.placeholder_icon_color }};
  }

  .ai-review-info-{{ ai_gen_id }} {
    flex-grow: 1;
  }

  .ai-review-name-{{ ai_gen_id }} {
    font-size: 16px;
    font-weight: 600;
    color: {{ block.settings.name_color }};
    margin: 0 0 4px;
  }

  .ai-review-date-{{ ai_gen_id }} {
    font-size: 14px;
    color: {{ block.settings.date_color }};
    margin: 0;
  }

  .ai-review-rating-{{ ai_gen_id }} {
    display: flex;
    gap: 2px;
    margin-bottom: 12px;
  }

  .ai-review-star-{{ ai_gen_id }} {
    width: 16px;
    height: 16px;
    color: {{ block.settings.star_color }};
  }

  .ai-review-star-empty-{{ ai_gen_id }} {
    color: {{ block.settings.star_empty_color }};
  }

  .ai-review-text-{{ ai_gen_id }} {
    font-size: 14px;
    line-height: 1.5;
    color: {{ block.settings.review_text_color }};
    margin-bottom: 16px;
  }

  .ai-review-media-{{ ai_gen_id }} {
    width: 100%;
    border-radius: {{ block.settings.media_border_radius }}px;
    overflow: hidden;
  }

  .ai-review-image-{{ ai_gen_id }} {
    width: 100%;
    height: 200px;
    object-fit: cover;
  }

  .ai-review-image-only-{{ ai_gen_id }} {
    height: 300px;
  }

  .ai-review-video-{{ ai_gen_id }} {
    width: 100%;
    height: 200px;
  }

  .ai-review-video-only-{{ ai_gen_id }} {
    height: 300px;
  }

  .ai-review-media-placeholder-{{ ai_gen_id }} {
    width: 100%;
    height: 200px;
    background-color: {{ block.settings.placeholder_color }};
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
  }

  .ai-review-media-placeholder-only-{{ ai_gen_id }} {
    height: 300px;
  }

  .ai-review-media-placeholder-{{ ai_gen_id }} svg {
    width: 48px;
    height: 48px;
    color: {{ block.settings.placeholder_icon_color }};
  }

  .ai-review-empty-state-{{ ai_gen_id }} {
    position: absolute;
    bottom: 20px;
    left: 50%;
    transform: translateX(-50%);
    font-size: 14px;
    color: {{ block.settings.placeholder_text_color }};
    text-align: center;
    pointer-events: none;
  }

  @media screen and (max-width: 768px) {
    .ai-reviews-section-{{ ai_gen_id }} {
      padding: {{ block.settings.section_padding_top | times: 0.7 }}px 0 {{ block.settings.section_padding_bottom | times: 0.7 }}px;
    }

    .ai-reviews-container-{{ ai_gen_id }} {
      padding: 0 {{ block.settings.section_padding_horizontal | times: 0.7 }}px;
    }

    .ai-reviews-grid-{{ ai_gen_id }} {
      grid-template-columns: 1fr;
      gap: {{ block.settings.card_gap | times: 0.75 }}px;
    }

    .ai-reviews-title-{{ ai_gen_id }} {
      font-size: {{ block.settings.title_size | times: 0.8 }}px;
    }

    .ai-review-card-{{ ai_gen_id }} {
      padding: {{ block.settings.card_padding_vertical | times: 0.75 }}px {{ block.settings.card_padding_horizontal | times: 0.75 }}px;
    }
  }
{% endstyle %}

<customer-reviews-{{ ai_gen_id }}
  class="ai-reviews-section-{{ ai_gen_id }}"
  {{ block.shopify_attributes }}
>
  <div class="ai-reviews-container-{{ ai_gen_id }}">
    {% if block.settings.title != blank or block.settings.subtitle != blank %}
      <div class="ai-reviews-header-{{ ai_gen_id }}">
        {% if block.settings.title != blank %}
          <h2 class="ai-reviews-title-{{ ai_gen_id }}">{{ block.settings.title }}</h2>
        {% endif %}
        {% if block.settings.subtitle != blank %}
          <p class="ai-reviews-subtitle-{{ ai_gen_id }}">{{ block.settings.subtitle }}</p>
        {% endif %}
      </div>
    {% endif %}

    <div class="ai-reviews-grid-{{ ai_gen_id }}">
      {% for i in (1..6) %}
        {% liquid
          assign review_type_key = 'review_' | append: i | append: '_type'
          assign review_enabled_key = 'review_' | append: i | append: '_enabled'
          assign review_name_key = 'review_' | append: i | append: '_name'
          assign review_date_key = 'review_' | append: i | append: '_date'
          assign review_rating_key = 'review_' | append: i | append: '_rating'
          assign review_text_key = 'review_' | append: i | append: '_text'
          assign review_image_key = 'review_' | append: i | append: '_image'
          assign review_video_key = 'review_' | append: i | append: '_video'
          assign review_avatar_key = 'review_' | append: i | append: '_avatar'

          assign review_type = block.settings[review_type_key]
          assign review_enabled = block.settings[review_enabled_key]
          assign review_name = block.settings[review_name_key]
          assign review_date = block.settings[review_date_key]
          assign review_rating = block.settings[review_rating_key]
          assign review_text = block.settings[review_text_key]
          assign review_image = block.settings[review_image_key]
          assign review_video = block.settings[review_video_key]
          assign review_avatar = block.settings[review_avatar_key]
        %}

        {% if review_enabled %}
          <div class="ai-review-card-{{ ai_gen_id }} {% if review_type == 'image_only' %}ai-review-card-image-only-{{ ai_gen_id }}{% elsif review_type == 'video_only' %}ai-review-card-video-only-{{ ai_gen_id }}{% endif %}">
            {% if review_type == 'full_review' %}
              <div class="ai-review-header-{{ ai_gen_id }}">
                <div class="ai-review-avatar-{{ ai_gen_id }}">
                  {% if review_avatar %}
                    <img
                      src="{{ review_avatar | image_url: width: 100 }}"
                      alt="{{ review_name | escape }}"
                      loading="lazy"
                      width="100"
                      height="100"
                    >
                  {% else %}
                    <div class="ai-review-avatar-placeholder-{{ ai_gen_id }}">
                      <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M16 7a4 4 0 11-8 0 4 4 0 018 0zM12 14a7 7 0 00-7 7h14a7 7 0 00-7-7z" />
                      </svg>
                    </div>
                  {% endif %}
                </div>
                <div class="ai-review-info-{{ ai_gen_id }}">
                  {% if review_name != blank %}
                    <h3 class="ai-review-name-{{ ai_gen_id }}">{{ review_name }}</h3>
                  {% endif %}
                  {% if review_date != blank %}
                    <p class="ai-review-date-{{ ai_gen_id }}">{{ review_date }}</p>
                  {% endif %}
                </div>
              </div>

              {% if review_rating > 0 %}
                <div class="ai-review-rating-{{ ai_gen_id }}">
                  {% for star in (1..5) %}
                    <svg class="ai-review-star-{{ ai_gen_id }} {% if star > review_rating %}ai-review-star-empty-{{ ai_gen_id }}{% endif %}" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor">
                      <path d="M12 2l3.09 6.26L22 9.27l-5 4.87 1.18 6.88L12 17.77l-6.18 3.25L7 14.14 2 9.27l6.91-1.01L12 2z"/>
                    </svg>
                  {% endfor %}
                </div>
              {% endif %}

              {% if review_text != blank %}
                <div class="ai-review-text-{{ ai_gen_id }}">{{ review_text }}</div>
              {% endif %}

              {% if review_image or review_video %}
                <div class="ai-review-media-{{ ai_gen_id }}">
                  {% if review_image %}
                    <img
                      src="{{ review_image | image_url: width: 600 }}"
                      alt="Review image"
                      class="ai-review-image-{{ ai_gen_id }}"
                      loading="lazy"
                      width="{{ review_image.width }}"
                      height="{{ review_image.height }}"
                    >
                  {% elsif review_video %}
                    <video
                      class="ai-review-video-{{ ai_gen_id }}"
                      controls
                      preload="metadata"
                    >
                      <source src="{{ review_video }}" type="video/mp4">
                      Your browser does not support the video tag.
                    </video>
                  {% endif %}
                </div>
              {% endif %}

            {% elsif review_type == 'image_only' %}
              {% if review_image %}
                <img
                  src="{{ review_image | image_url: width: 600 }}"
                  alt="Customer review image"
                  class="ai-review-image-{{ ai_gen_id }} ai-review-image-only-{{ ai_gen_id }}"
                  loading="lazy"
                  width="{{ review_image.width }}"
                  height="{{ review_image.height }}"
                >
              {% else %}
                <div class="ai-review-media-placeholder-{{ ai_gen_id }} ai-review-media-placeholder-only-{{ ai_gen_id }}">
                  <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 16l4.586-4.586a2 2 0 012.828 0L16 16m-2-2l1.586-1.586a2 2 0 012.828 0L20 14m-6-6h.01M6 20h12a2 2 0 002-2V6a2 2 0 00-2-2H6a2 2 0 00-2 2v12a2 2 0 002 2z" />
                  </svg>
                  <div class="ai-review-empty-state-{{ ai_gen_id }}">
                    Add a review image
                  </div>
                </div>
              {% endif %}

            {% elsif review_type == 'video_only' %}
              {% if review_video %}
                <video
                  class="ai-review-video-{{ ai_gen_id }} ai-review-video-only-{{ ai_gen_id }}"
                  controls
                  preload="metadata"
                >
                  <source src="{{ review_video }}" type="video/mp4">
                  Your browser does not support the video tag.
                </video>
              {% else %}
                <div class="ai-review-media-placeholder-{{ ai_gen_id }} ai-review-media-placeholder-only-{{ ai_gen_id }}">
                  <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 10l4.553-2.276A1 1 0 0121 8.618v6.764a1 1 0 01-1.447.894L15 14M5 18h8a2 2 0 002-2V8a2 2 0 00-2-2H5a2 2 0 00-2 2v8a2 2 0 002 2z" />
                  </svg>
                  <div class="ai-review-empty-state-{{ ai_gen_id }}">
                    Add a review video
                  </div>
                </div>
              {% endif %}
            {% endif %}
          </div>
        {% endif %}
      {% endfor %}
    </div>
  </div>
</customer-reviews-{{ ai_gen_id }}>

<script>
  (function() {
    class CustomerReviews{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
      }

      connectedCallback() {
        this.setupVideoControls();
      }

      setupVideoControls() {
        const videos = this.querySelectorAll('.ai-review-video-{{ ai_gen_id }}');
        
        videos.forEach(video => {
          video.addEventListener('loadedmetadata', () => {
            video.style.aspectRatio = `${video.videoWidth} / ${video.videoHeight}`;
          });

          video.addEventListener('error', () => {
            console.warn('Video failed to load:', video.src);
          });
        });
      }
    }

    customElements.define('customer-reviews-{{ ai_gen_id }}', CustomerReviews{{ ai_gen_id }});
  })();
</script>

